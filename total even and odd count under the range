even_count=0
odd_count=0
for i in range(1,11):
    if(i%2==0):
        even_count+=1
        print("the total even numbers is:",even_count)
    else:
         odd_count+=1
         print("the total number of odd numbers is :",odd_count)

print("final_count:")
print("the even count is:",even_count)
print("the odd count is:",odd_count)
